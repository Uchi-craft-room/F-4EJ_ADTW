  </animation>



<!-- RADAR SYMBOL -->


<!-- SYMBOL_1 -->

   <animation>
     <type>select</type>
   <object-name>T1</object-name>
        <condition>
         <and>
         <property>ai/models/tanker/radar/in-range</property>
         <or>
            <property>controls/electric/engine/generator</property>
            <property>controls/electric/engine[1]/generator</property>
         </or>
         <greater-than>
            <property>systems/electrical/volts</property>
            <value>24</value>
         </greater-than>
         <greater-than>
            <property>ai/models/tanker/radar/h-offset</property>
            <value>-60</value>
         </greater-than>
         <less-than>
            <property>ai/models/tanker/radar/h-offset</property>
            <value>60</value>
         </less-than>
         <less-than>
            <property>ai/models/tanker/radar/range-nm</property>
            <property>instrumentation/radar/range</property>
         </less-than>
         </and>
        </condition>
  </animation>

  <animation>
    <type>translate</type>
     <object-name>T1</object-name>
     <object-name>cursol1</object-name>
     <property>ai/models/tanker/radar/range-nm-shift</property>
        <axis>
            <x> 0.0 </x>
            <y> 0.0 </y>
            <z> 1.0 </z>
        </axis>
        <center>
            <x-m>0.0</x-m>
            <y-m>0.0</y-m>
            <z-m>0.0</z-m>
        </center>
  </animation>

  <animation>
    <type>translate</type>
     <object-name>T1</object-name>
