   <animation>
     <type>select</type>
   <object-name>range2</object-name>
        <condition>
         <greater>
            <property>instrumentation/radar/range</property>
            <value>10</value>
         </greater>
        </condition>
  </animation>


  <animation>
     <type>select</type>
   <object-name>range3</object-name>
