<?xml version="1.0"?>

<PropertyList>

    <path>tacan.ac</path>

    <animation>
	<type>range</type>
	<min-m>0</min-m>
	<max-m>10</max-m>
    </animation>
 
  <animation>
         <object-name>base</object-name>
         <object-name>tacan.pwr</object-name>
         <object-name>SW-X</object-name>
         <object-name>SW-Y</object-name>
         <object-name>chan.knob</object-name>
         <object-name>chan.001</object-name>
         <object-name>chan.002</object-name>
         <object-name>chan.003</object-name>
        <object-name>freq.001</object-name>
        <object-name>freq.002</object-name>
        <object-name>freq.003</object-name>
        <object-name>freq.004</object-name>
        <object-name>freq.005</object-name>
        <object-name>ils.vol</object-name>
     <object-name>freq-A.knob</object-name>
     <object-name>freq-B.knob</object-name>
     <type>material</type>
     <emission> 
      <factor-prop>systems/electrical/outputs/instrument-lights-norm</factor-prop>
        <red>0.5</red>
        <green>0.2</green>
        <blue>0.2</blue>
      </emission>
  </animation> 



  <animation>
  <object-name>xmt.annun</object-name>
  <object-name>xmt.annun.001</object-name>
   <type>material</type>
     <emission>
        <red>  1.0 </red>
        <green>1.0 </green>
        <blue> 1.0 </blue>
     </emission>
  </animation> 



<!-- TACAN -->
    <animation>
        <type>select</type>
        <object-name>xmt.annun</object-name>
        <condition>
             <and>
             <not>
               <equals>
                <property>instrumentation/tacan/display/x-shift</property>
                <value> 0 </value>
               </equals>
             </not>
             <property>instrumentation/tacan/serviceable</property>
	     <greater-than>
		<property>systems/electrical/volts</property>
		<value>24</value>
	      </greater-than>
             </and>
        </condition>
    </animation>  

    <animation>
        <type>rotate</type>
        <object-name>tacan.pwr</object-name>
        <property>instrumentation/tacan/serviceable</property>
        <interpolation>
            <entry><ind>0</ind><dep>0</dep></entry>
            <entry><ind>1.0</ind><dep>120</dep></entry>
        </interpolation>
        <center>
            <x-m>0.008</x-m>
            <y-m>0.052</y-m>
            <z-m>-0.021</z-m>
        </center>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>


    <animation>
        <type>rotate</type>
        <object-name>chan.knob</object-name>
        <property>instrumentation/tacan/frequencies/selected-channel</property>
        <factor>3</factor>
        <center>
            <x-m>0.011</x-m>
            <y-m>-0.030</y-m>
            <z-m>-0.021</z-m>
        </center>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>


    <animation>
        <type>select</type>
        <object-name>SW-X</object-name>
        <condition>
            <equals>
                <property>instrumentation/tacan/frequencies/selected-channel[4]</property>
                <value>X</value>
            </equals>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>SW-Y</object-name>
        <condition>
            <equals>
                <property>instrumentation/tacan/frequencies/selected-channel[4]</property>
                <value>Y</value>
            </equals>
        </condition>
    </animation>


<!--    Active Frequency   -->

    <animation>
        <type>textranslate</type>
        <object-name>chan.001</object-name>
        <property>instrumentation/tacan/frequencies/selected-channel[1]</property>
        <factor>0.1</factor>
        <offset>-0.0</offset>
        <axis>
            <x>0</x>
            <y>-1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>chan.002</object-name>
        <property>instrumentation/tacan/frequencies/selected-channel[2]</property>
        <factor>0.1</factor>
        <offset>-0.0</offset>
        <axis>
            <x>0</x>
            <y>-1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>chan.003</object-name>
        <property>instrumentation/tacan/frequencies/selected-channel[3]</property>
        <factor>0.1</factor>
        <offset>-0.0</offset>
        <axis>
            <x>0</x>
            <y>-1</y>
            <z>0</z>
        </axis>
    </animation>


    <animation>
        <type>pick</type>
        <object-name>pwr.off</object-name>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
            <command>property-assign</command>
            <property>instrumentation/tacan/serviceable</property>
            <value>false</value>
            </binding>
      <binding>
        <command>property-toggle</command>
        <property>sim/sound/click</property>
      </binding>
        </action>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>pwr.on</object-name>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-assign</command>
                <property>instrumentation/tacan/serviceable</property>
                <value>true</value>
            </binding>
      <binding>
        <command>property-toggle</command>
        <property>sim/sound/click</property>
      </binding>
        </action>
    </animation>


    <animation>
        <type>pick</type>
        <object-name>freq-X</object-name>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-assign</command>
                <property>instrumentation/tacan/frequencies/selected-channel[4]</property>
                <value>X</value>
            </binding>
      <binding>
        <command>property-toggle</command>
        <property>sim/sound/click</property>
      </binding>
        </action>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>freq-Y</object-name>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-assign</command>
                <property>instrumentation/tacan/frequencies/selected-channel[4]</property>
                <value>Y</value>
            </binding>
      <binding>
        <command>property-toggle</command>
        <property>sim/sound/click</property>
      </binding>
        </action>
    </animation>



    <animation>
        <type>pick</type>
        <object-name>chan.knob</object-name>
        <action>
            <button>0</button>
            <repeatable>true</repeatable>
            <interval-sec>0.2</interval-sec>
            <binding>
                <command>property-adjust</command>
                <property>instrumentation/tacan/frequencies/selected-channel</property>
                <step>10</step>
                <min>0</min>
                <max>126</max>
                <wrap>true</wrap>
            </binding>
        </action>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>chan.knob</object-name>
        <action>
            <button>1</button>
            <repeatable>true</repeatable>
            <interval-sec>0.2</interval-sec>
            <binding>
                <command>property-adjust</command>
                <property>instrumentation/tacan/frequencies/selected-channel</property>
                <step>-10</step>
                <min>0</min>
                <max>126</max>
                <wrap>true</wrap>
            </binding>
        </action>
    </animation>



    <animation>
        <type>pick</type>
        <object-name>chan.knob</object-name>
        <action>
            <button>4</button>
            <repeatable>true</repeatable>
            <interval-sec>0.2</interval-sec>
            <binding>
                <command>property-adjust</command>
                <property>instrumentation/tacan/frequencies/selected-channel</property>
                <step>1</step>
                <min>0</min>
                <max>126</max>
                <wrap>true</wrap>
            </binding>
        </action>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>chan.knob</object-name>
        <action>
            <button>3</button>
            <repeatable>true</repeatable>
            <interval-sec>0.2</interval-sec>
            <binding>
                <command>property-adjust</command>
                <property>instrumentation/tacan/frequencies/selected-channel</property>
                <step>-1</step>
                <min>0</min>
                <max>126</max>
                <wrap>true</wrap>
            </binding>
        </action>
    </animation>



<!-- ************************ ILS/VOR *************************** -->

<!-- Freq. range 108.000 - 117.975 (normal) 
     Strap option 133.00 - 135.95
     05 khz spacing -->


    <animation>
        <type>select</type>
        <object-name>xmt.annun.001</object-name>
        <condition>
                <property>instrumentation/nav/data-is-valid</property>
        </condition>
    </animation>  

    <animation>
        <type>rotate</type>
        <object-name>ils.vol</object-name>
        <property>instrumentation/nav/serviceable</property>
        <interpolation>
            <entry><ind>0</ind><dep>0</dep></entry>
            <entry><ind>1.0</ind><dep>30</dep></entry>
        </interpolation>
        <center>
            <x-m>0.008</x-m>
            <y-m>0.052</y-m>
            <z-m>0.026</z-m>
        </center>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>ils.vol</object-name>
        <property>instrumentation/nav[0]/volume</property>
        <interpolation>
            <entry><ind>0</ind><dep>0</dep></entry>
            <entry><ind>1.0</ind><dep>270</dep></entry>
        </interpolation>
        <center>
            <x-m>0.008</x-m>
            <y-m>0.052</y-m>
            <z-m>0.026</z-m>
        </center>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

<!--    Active Frequency   -->

    <animation>
        <type>textranslate</type>
        <object-name>freq.001</object-name>
        <property>instrumentation/VIR-32/nav1</property>
        <factor>0.00001</factor>
        <step>10000</step>
        <axis>
            <x>0</x>
            <y>-1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>freq.002</object-name>
        <property>instrumentation/VIR-32/nav1</property>
        <factor>0.0001</factor>
        <step>1000</step>
        <axis>
            <x>0</x>
            <y>-1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>freq.003</object-name>
        <property>instrumentation/VIR-32/nav1</property>
        <factor>0.001</factor>
        <step>100</step>
        <axis>
            <x>0</x>
            <y>-1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>freq.004</object-name>
        <property>instrumentation/VIR-32/nav1</property>
        <factor>0.01</factor>
        <step>10</step>
        <axis>
            <x>0</x>
            <y>-1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>freq.005</object-name>
        <property>instrumentation/VIR-32/nav1</property>
        <factor>0.1</factor>
        <step>1</step>
        <axis>
            <x>0</x>
            <y>-1</y>
            <z>0</z>
        </axis>
    </animation>


    <animation>
        <type>pick</type>
        <object-name>pwr.off.001</object-name>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
            <command>property-assign</command>
            <property>instrumentation/nav/serviceable</property>
            <value>0</value>
            </binding>
            <binding>
            <command>property-assign</command>
            <property>instrumentation/nav/ident</property>
            <value>false</value>
        </binding>
        </action>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>ils.vol</object-name>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-assign</command>
                <property>instrumentation/nav/serviceable</property>
                <value>1</value>
            </binding>
            <binding>
            <command>property-assign</command>
            <property>instrumentation/nav/ident</property>
            <value>true</value>
        </binding>
        </action>
    </animation>


    <animation>
        <type>pick</type>
       <object-name>ils.vol</object-name>
        <action>
            <button>3</button>
            <repeatable>true</repeatable>
            <interval-sec>0.2</interval-sec>
            <binding>
                <command>property-adjust</command>
                <property>instrumentation/nav[0]/volume</property>
                <step>-0.1</step>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>

    <animation>
        <type>pick</type>
       <object-name>ils.vol</object-name>
        <action>
            <button>4</button>
            <repeatable>true</repeatable>
            <interval-sec>0.2</interval-sec>
            <binding>
                <command>property-adjust</command>
                <property>instrumentation/nav[0]/volume</property>
                <step>0.1</step>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>freq-A.knob</object-name>
        <action>
            <button>3</button>
            <repeatable>true</repeatable>
            <interval-sec>0.2</interval-sec>
            <binding>
                <command>property-adjust</command>
                <property>instrumentation/VIR-32/nav1</property>
                <step>-5</step>
                <min>10800</min>
                <max>11800</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>freq-A.knob</object-name>
        <action>
            <button>4</button>
            <repeatable>true</repeatable>
            <interval-sec>0.2</interval-sec>
            <binding>
                <command>property-adjust</command>
                <property>instrumentation/VIR-32/nav1</property>
                <step>5</step>
                <min>10800</min>
                <max>11800</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>freq-B.knob</object-name>
        <action>
            <button>3</button>
            <repeatable>true</repeatable>
            <interval-sec>0.2</interval-sec>
            <binding>
                <command>property-adjust</command>
                <property>instrumentation/VIR-32/nav1</property>
                <step>-100</step>
                <min>10800</min>
                <max>11800</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>freq-B.knob</object-name>
        <action>
            <button>4</button>
            <repeatable>true</repeatable>
            <interval-sec>0.2</interval-sec>
            <binding>
                <command>property-adjust</command>
                <property>instrumentation/VIR-32/nav1</property>
                <step>100</step>
                <min>10800</min>
                <max>11800</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>


    <animation>
        <type>rotate</type>
        <object-name>freq-A.knob</object-name>
        <property>instrumentation/VIR-32/nav1</property>
        <interpolation>
            <entry><ind>10800</ind><dep>0</dep></entry>
            <entry><ind>10900</ind><dep>360</dep></entry>
            <entry><ind>11000</ind><dep>720</dep></entry>
            <entry><ind>11100</ind><dep>1080</dep></entry>
            <entry><ind>11200</ind><dep>1440</dep></entry>
            <entry><ind>11300</ind><dep>1800</dep></entry>
            <entry><ind>11400</ind><dep>2160</dep></entry>
            <entry><ind>11500</ind><dep>2520</dep></entry>
            <entry><ind>11600</ind><dep>2880</dep></entry>
            <entry><ind>11700</ind><dep>3240</dep></entry>
            <entry><ind>11800</ind><dep>3600</dep></entry>
        </interpolation>
        <center>
            <x-m>0.016</x-m>
            <y-m>-0.053</y-m>
            <z-m>0.026</z-m>
        </center>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>



    <animation>
        <type>rotate</type>
        <object-name>freq-B.knob</object-name>
        <property>instrumentation/VIR-32/nav1</property>
 	<interpolation>
            <entry><ind>10800</ind><dep>0</dep></entry>
            <entry><ind>11795</ind><dep>180</dep></entry>
        </interpolation>
        <center>
            <x-m>0.016</x-m>
            <y-m>-0.053</y-m>
            <z-m>0.026</z-m>
        </center>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

</PropertyList>
