<!--  volume  -->

    <animation>
        <type>rotate</type>
        <object-name>vol-knob</object-name>
        <property>instrumentation/comm/volume</property>
        <interpolation>
            <entry><ind>0</ind><dep>0</dep></entry>
            <entry><ind>1.0</ind><dep>270</dep></entry>
        </interpolation>
        <center>
            <x-m>0.009</x-m>
            <y-m>0.063</y-m>
            <z-m>-0.011</z-m>
        </center>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>pick</type>
       <object-name>vol-knob</object-name>
        <action>
            <button>3</button>
            <repeatable>true</repeatable>
            <binding>
                <command>property-adjust</command>
                <property>instrumentation/comm/volume</property>
                <step>-0.1</step>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>

    <animation>
        <type>pick</type>
       <object-name>vol-knob</object-name>
        <action>
            <button>4</button>
            <repeatable>true</repeatable>
            <binding>
                <command>property-adjust</command>
                <property>instrumentation/comm/volume</property>
                <step>0.1</step>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
