<animation>
  <type>rotate</type>
  <object-name>bingo-knob</object-name>
  <property>/consumables/fuel/bingo-lbs</property>
  <factor>0.01</factor>
  <center>
   <x-m>0.006</x-m>
   <y-m>-0.025</y-m>
   <z-m>0.026</z-m>
  </center>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

<animation>
  <type>rotate</type>
  <object-name>bingo</object-name>
  <property>/consumables/fuel/bingo-lbs</property>
    <interpolation>
   <entry><ind>0</ind><dep>0</dep></entry>
   <entry><ind>14000</ind><dep>200</dep></entry>
  </interpolation>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>0</z-m>
  </center>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>


