<!--  bingo setting -->

  <animation>
    <type>pick</type>
    <object-name>bingo-knob</object-name>
    <action>
      <button>0</button>
      <repeatable>true</repeatable>
      <interval-sec>0.1</interval-sec>
      <binding>
        <command>property-adjust</command>
        <property>/consumables/fuel/bingo-lbs</property>
        <step>  1000 </step>
        <min>  0 </min>
        <max>  14000 </max>
      </binding>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>bingo-knob</object-name>
    <action>
      <button>1</button>
      <repeatable>true</repeatable>
      <interval-sec>0.1</interval-sec>
      <binding>
        <command>property-adjust</command>
        <property>/consumables/fuel/bingo-lbs</property>
        <step>  -1000 </step>
        <min>  0 </min>
        <max>  14000 </max>
      </binding>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>bingo-knob</object-name>
    <action>
      <button>3</button>
